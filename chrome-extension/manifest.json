{  
  "manifest_version": 2,  
  "name": "Add to read webpage list",  
  "description": "Extension to create a context menu to add current webpage to read webpage list.",  
  "version": "2.0",  
  "permissions": [
    "contextMenus"
  ],  
  "background": {  
    "scripts": [
      "event.js"
    ],  
    "persistent": true  
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}